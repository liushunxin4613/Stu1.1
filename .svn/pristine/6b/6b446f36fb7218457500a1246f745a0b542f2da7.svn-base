<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    tools:context="com.fengyang.stu.PublishSecondActivity$PlaceholderFragment" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/view_margin"
            android:background="@drawable/input_kuang"
            android:focusable="true"
            android:focusableInTouchMode="true"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    style="@style/publish_item_title_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Et_item_authentication_sell_name" />

                <EditText
                    android:id="@+id/Et_publish_part_input_sell_name"
                    style="@style/publish_editText_style"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:imeOptions="actionNext"
                    android:maxLength="80"
                    android:singleLine="true" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginBottom="4dp"
                android:layout_marginTop="4dp"
                android:background="@color/line_gray" />

            <LinearLayout
                android:id="@+id/Ly_authentication_user_seller_sex"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@drawable/item_selector"
                android:clickable="true"
                android:gravity="center_vertical"
                android:orientation="horizontal" >

                <TextView
                    style="@style/publish_item_title_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Et_item_authentication_sell_sex" />

                <TextView
                    android:id="@+id/Tv_authentication_user_seller_sex"
                    style="@style/publish_editText_style"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:layout_marginStart="4dp"
                    android:layout_weight="1"
                    android:maxLength="80"
                    android:singleLine="true" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="4dp"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:layout_marginStart="4dp"
                    android:background="@drawable/info_more"
                    android:contentDescription="@string/image_string" />
            </LinearLayout>

            <View
                style="@style/item_line_edit_style"
                android:layout_width="match_parent"
                android:layout_height="1px" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    style="@style/publish_item_title_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Et_item_authentication_sell_tname" />

                <EditText
                    android:id="@+id/Et_publish_part_input_sell_tname"
                    style="@style/publish_editText_style"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:imeOptions="actionNext"
                    android:maxLength="80"
                    android:singleLine="true" />
            </LinearLayout>

            <View
                style="@style/item_line_edit_style"
                android:layout_width="match_parent"
                android:layout_height="1px" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    style="@style/publish_item_title_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Et_item_authentication_idcard" />

                <EditText
                    android:id="@+id/Et_item_authentication_sell_idcard"
                    style="@style/publish_editText_style"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:imeOptions="actionNext"
                    android:maxLength="80"
                    android:singleLine="true" />
            </LinearLayout>

            <View
                style="@style/item_line_edit_style"
                android:layout_width="match_parent"
                android:layout_height="1px" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal" >

                <TextView
                    style="@style/publish_item_title_style"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/Et_item_authentication_sell_card" />

                <EditText
                    android:id="@+id/Et_item_authentication_sell_card"
                    style="@style/publish_editText_style"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:imeOptions="actionNext"
                    android:inputType="numberDecimal"
                    android:maxLength="80"
                    android:singleLine="true" />
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/view_margin"
            android:background="@drawable/input_kuang"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_horizontal"
                android:orientation="horizontal"
                android:padding="@dimen/view_margin_small" >

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical" >

                    <TextView
                        style="@style/publish_item_title_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxLines="1"
                        android:text="@string/Tv_item_authentication_man_photoupcard" />

                    <com.fengyang.customLib.SquareLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp" >

                        <ImageButton
                            android:id="@+id/authentication_choose_idCard_photo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/photo_frame_selector"
                            android:contentDescription="@string/image_string"
                            android:scaleType="centerCrop"
                            android:src="@drawable/ic_take_photo" />
                    </com.fengyang.customLib.SquareLayout>
                </LinearLayout>

                <View
                    android:layout_width="1px"
                    android:layout_height="match_parent"
                    android:layout_marginLeft="4dp"
                    android:layout_marginRight="4dp"
                    android:background="@color/line_gray" />

                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:orientation="vertical" >

                    <TextView
                        style="@style/publish_item_title_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:maxLines="1"
                        android:text="@string/Tv_item_authentication_sell_photo" />

                    <com.fengyang.customLib.SquareLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:padding="10dp" >

                        <ImageButton
                            android:id="@+id/authentication_choose_license_photo"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:background="@drawable/photo_frame_selector"
                            android:contentDescription="@string/image_string"
                            android:scaleType="centerCrop"
                            android:src="@drawable/ic_take_photo" />
                    </com.fengyang.customLib.SquareLayout>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/view_margin"
            android:background="@drawable/input_kuang"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <LinearLayout
                    android:id="@+id/publish_authentication_sell_location"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@drawable/item_selector"
                    android:clickable="true"
                    android:gravity="center_vertical"
                    android:orientation="horizontal" >

                    <TextView
                        style="@style/publish_editText_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/Tv_item_authentication_sell_selleraddress" />

                    <TextView
                        android:id="@+id/Tv_publish_authentication_sell_location_show"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1"
                        android:paddingBottom="@dimen/view_margin_small"
                        android:paddingTop="@dimen/view_margin_small" />

                    <ImageView
                        style="@style/item_more_image_style"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="@string/image_string" />
                </LinearLayout>

                <View
                    style="@style/item_line_edit_style"
                    android:layout_width="match_parent"
                    android:layout_height="1px" />

                <EditText
                    android:id="@+id/Et_publish_sell_input_place"
                    style="@style/publish_editText_style"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="@string/Tv_item_authentication_sell_selleraddress_detail"
                    android:imeOptions="actionNext"
                    android:minLines="2" />
            </LinearLayout>

            
        </LinearLayout>

        <Button
            android:id="@+id/Bt_authentication_sell_submit"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="@dimen/view_margin_large"
            android:layout_marginLeft="@dimen/view_margin"
            android:layout_marginRight="@dimen/view_margin"
            android:background="@drawable/button_blue_selector"
            android:text="@string/Bt_item_authentication_submit"
            android:textColor="@color/white" />
    </LinearLayout>

</ScrollView>